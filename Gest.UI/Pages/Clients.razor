@page "/clients"
@layout AdminLayout

<PageTitle>Clients</PageTitle>
<TopAdminNav Title="Gestion des clients" />
<div class="h-full mt-16 p-0 px-2" style="padding:0rem;">
    <h1>Clients en cours de developement</h1>
</div>
@code {

    [Inject] AdminNavState AdminNavState { get; set; } = null!;
  

    protected override Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            AdminNavState.CurrentUrl = UrlKeys.Clients;
        }
        return base.OnAfterRenderAsync(firstRender);
    }
}