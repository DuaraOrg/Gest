@page "/users"
@layout AdminLayout

@using Gest.UI._Features_.Users.Components
<PageTitle>Utilisateurs</PageTitle>
<TopAdminNav Title="Gestion des utilisateurs" />
<div class="h-full mt-16 p-0 px-2" style="padding:0rem;">
    <List/>
</div>
@code {
     [Inject] AdminNavState AdminNavState { get; set; } = null!;
  

    protected override Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            AdminNavState.CurrentUrl = UrlKeys.Users;
        }
        return base.OnAfterRenderAsync(firstRender);
    }
}
